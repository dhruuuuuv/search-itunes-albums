{
  "name": "webpack-marionette",
  "description": "A small boilerplate introducing webpack and es6 features to a Marionette/Backbone application",
  "author": "alexpsi",
  "version": "0.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpsi/webpack-marionette"
  },
  "scripts": {
    "build": "rimraf dist && webpack --config webpack-prod.config.js --colors",
    "analyze": "webpack --config webpack-prod.config.js --json | webpack-bundle-size-analyzer",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors",
    "test": "karma start --single-run",
    "tdd": "karma start",
    "json": "json-server --watch ./app/db.json",
    "eject": "rimraf components/* && rimraf routes/*",
    "dash": "webpack-dashboard -- webpack-dev-server --config webpack-dev.config.js"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-polyfill": "^6.9.0",
    "backbone": "^1.3.3",
    "backbone-forms": "^0.14.1",
    "backbone.marionette": "^3.1.0",
    "backbone.paginator": "^2.0.5",
    "jquery": "^3.1.1",
    "marked": "^0.3.6",
    "milligram": "^1.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.1",
    "isparta": "^4.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.7.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "underscore-template-loader": "^0.7.2",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
}
